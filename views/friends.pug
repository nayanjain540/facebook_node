	body
		p#none
		<button onclick="none()">Double Click For A Blank Slate</button>
		<button onclick="totfriends()">TotalFriends</button>
		<button onclick="recfriends()">RecievedFriends</button>
		<button onclick="rejfriends()">RejectedFriends</button>
		<button onclick="delfriends()">DeletedFriends</button>
		<button onclick="sentfriends()">SentFriends</button>
		<button onclick="numstats()">CLICK FOR STATS</button>
		#table-wrapper
			table#totfriends 
				tr
					th FriendsName
					th TimestampofAddition
				each j,i in articles.total_friends
					tr
						td=articles.total_friends[i][0]
						td=articles.total_friends[i][1]
			table#recfriends 
				tr
					th RecievedFriendsName
					th TimestampofAddition
				each j,i in articles.received_friends
					tr
						td=articles.received_friends[i][0]
						td=articles.received_friends[i][1]
			table#rejfriends 
				tr
					th RejecteddFriendsName
					th TimestampofRemoval
				each j,i in articles.rejected_friends
					tr
						td=articles.rejected_friends[i][0]
						td=articles.rejected_friends[i][1]									
			table#delfriends 
				tr
					th DeletedFriendsName
					th TimestampofRemoval
				each j,i in articles.deleted_friends
					tr
						td=articles.deleted_friends[i][0]
						td=articles.deleted_friends[i][1]
			table#sentfriends 
				tr
					th SentFriendsName
					th TimestampofAdded
				each j,i in articles.sent_requests
					tr
						td=articles.sent_requests[i][0]
						td=articles.sent_requests[i][1]	
			table#numstats
				tr
					td LengthOfFriends
					td LengthofRecieved
					td LengthofRejected
					td LengthofDeleted
					td LengthofSent
				tr
					td=articles.lengthoffriends								
					td=articles.lengthofreceived
					td=articles.lengthofrejected
					td=articles.lengthofdeleted
					td=articles.lengthofsent