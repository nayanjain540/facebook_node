html
	head
		include mystyle.css
	script.
		function none() {
			x=document.getElementById('none');
			if (x.style.display === "none") {
				document.getElementById('totfriends').style.display="none";
				document.getElementById('recfriends').style.display="none";
				document.getElementById('rejfriends').style.display="none";
				document.getElementById('delfriends').style.display="none";
				document.getElementById('sentfriends').style.display="none";
				document.getElementById('numstats').style.display="none";
				x.style.display = "none";
			} 
			else 
			{
			x.style.display = "none";
			}
			}
		function numstats() {
			x=document.getElementById('numstats');
			if (x.style.display === "none") {
				document.getElementById('totfriends').style.display="none";
				document.getElementById('recfriends').style.display="none";
				document.getElementById('rejfriends').style.display="none";
				document.getElementById('delfriends').style.display="none";
				document.getElementById('sentfriends').style.display="none";
				x.style.display = "block";
			}
			else
			{
			x.style.display = "none";
			}
			}	
